{
    "microserviceId": "{{id}}",
{{#unless hasUIPrefix}}
    "isRooted": true,
    "routeSegment": "{{lowerCase name}}",
{{/unless}}
{{#if hasUIPrefix}}
    "isRooted": false,
    "routeSegment": "",
{{/if}}
