{
    "name": "vscode",
    "displayName": "vscode",
    "version": "9.22.2",
    "description": "",
    "author": "Dolittle",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCustomEditor:dolittle.application",
        "onCustomEditor:dolittle.features"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "dolittle.application",
                "displayName": "Dolittle Application",
                "selector": [
                    {
                        "filenamePattern": "application.json"
                    }
                ]
            },
            {
                "viewType": "dolittle.features",
                "displayName": "Dolittle Features",
                "selector": [
                    {
                        "filenamePattern": "features.json"
                    }
                ]
            }

        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "clean": "tsc -b --clean",
        "compile": "webpack",
        "start:dev": "concurrently \"yarn --cwd ../Visual start:vscode\" \"webpack --watch\"",
        "build": "webpack --mode production --devtool hidden-source-map",
        "lint": "eslint --cache '**/*.{js,ts,tsx}' --quiet --fix",
        "lint:ci": "eslint --cache '**/*.{js,ts,tsx}' --quiet",
        "test": "mocha",
        "ci": "yarn clean && yarn lint:ci && yarn build && yarn test"
    },
    "devDependencies": {
        "@types/vscode": "^1.56.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0",
        "ts-loader": "^8.0.14",
        "webpack": "^5.19.0",
        "webpack-cli": "^4.4.0"
    }
}
