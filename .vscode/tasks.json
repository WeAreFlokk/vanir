// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "start:dev",
            "label": "VSCodeExtension Watch",
            "options": {
                "cwd": "${workspaceFolder}/Source/Tooling/vscode"
            },
			"problemMatcher": [
                "$ts-webpack-watch",
                "$tslint-webpack-watch"
			],
			"isBackground": true,
			"group": "build",
			"presentation": {
				"reveal": "always"
			}
		}
	]
}
